{
  "name": "chat Hub",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch server.ts",
    "dev:node": "node --loader ts-node/esm server.ts",
    "dev:alt": "ts-node --esm --experimental-specifier-resolution=node server.ts",
    "dev:watch": "nodemon --exec \"tsx\" server.ts",
    "dev:build": "concurrently \"tsc --watch\" \"nodemon dist/server.js\"",
    "warning": "node --trace-warnings"
  },
